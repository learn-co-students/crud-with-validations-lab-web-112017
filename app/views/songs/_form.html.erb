<%= form_for(@song) do |f| %>
  <%=  f.text_field :title, placeholder: "Title" %><br>
  <%=  f.check_box :released %><br>
  <%=  f.text_field :release_year, placeholder: "Release Year" %><br>
  <%=  f.text_field :artist_name, placeholder: "Artist" %><br>
  <%=  f.text_field :genre, placeholder: "Genre" %><br>

  <%= f.submit %>
<% end %>
