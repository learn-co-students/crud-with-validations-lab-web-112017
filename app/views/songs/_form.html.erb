<h1>Song form</h1>

<%= form_for @song do |f| %>
<%= f.label :title%>
<%= f.text_field :title %> <br>
<%= f.label :released%>
<%= f.check_box :released %> <br>
<%= f.label :release_year%>
<%= f.number_field :release_year %> <br>
<%= f.label "Artist Name"%>
<%= f.text_field :artist_name %> <br>
<%= f.label "Genre"%>
<%= f.text_field :genre %> <br>

<%= f.submit %>

<%end%>
